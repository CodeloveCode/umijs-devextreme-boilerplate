{
  "private": true,
  "homepage": "https://gitee.com/mudking/react-umijs-devextreme",
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.2.2",
    "@ant-design/pro-layout": "^6.5.0",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.2.20",
    "axios": "^0.20.0",
    "devextreme": "20.1.7",
    "devextreme-react": "20.1.7",
    "exceljs": "^4.2.0",
    "file-saver": "^2.0.5",
    "jquery": "3.5.1",
    "lint-staged": "^10.0.7",
    "lodash": "^4.17.20",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "redux-logger": "^3.0.6",
    "umi": "^3.2.20",
    "yorkie": "^2.0.0"
  },
  "devDependencies": {
    "@types/file-saver": "^2.0.1",
    "@types/mockjs": "^1.0.3",
    "@types/redux-logger": "^3.0.8",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "eslint": "^7.9.0",
    "eslint-plugin-react": "^7.20.6",
    "mockjs": "^1.1.0",
    "raw-loader": "^4.0.1"
  }
}
